{
  "$schema": "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=(localdb)\\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\\Umbraco.mdf;Integrated Security=True"
  },
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": true
      },
      "Global": {
        "Id": "9208e5c1-83ec-40a6-8396-b59d157db166"
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeManual",
        "ModelsDirectory": "~/../Application.Models/CmsModels/Generated/",
        "AcceptUnsafeModelsDirectory": true,
        "ModelsNamespace": "Application.Models.CmsModels",
        "DebugLevel": 0
      }
    }
  },
  "uBase": {
    "ServiceCacheEnabled": true
  }
}